        fore: #AB37F2;
}

select_style {
        back: #03087D;
}

string_style {
        fore: #FF3AFF;
        modifiers: bold;
}

stringeol_style {
        fore: #000000;
        back: #EEC0EE;
        size: %(size)d;
        modifiers: bold eol;
}

unknown_style {
        fore: #FFFFFF;
        back: #DD0101;
        modifiers: bold eol;
}
